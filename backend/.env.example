# AWS Configuration
AWS_REGION=us-east-1
DYNAMODB_TABLE=footprint-table-dev

# Environment
ENVIRONMENT=dev

# JWT Configuration
JWT_SECRET=change-this-to-a-secure-random-string-in-production

# Apple Sign In
APPLE_BUNDLE_ID=com.wd.footprint.app

# Google Sign In (for authentication)
# Web client ID - used for verifying ID tokens from iOS app
GOOGLE_CLIENT_ID=your-web-client-id.apps.googleusercontent.com
# iOS client ID - used to validate tokens from iOS app (optional, for audience validation)
GOOGLE_IOS_CLIENT_ID=your-ios-client-id.apps.googleusercontent.com

# Google OAuth credentials (for Gmail/Calendar import - uses web client)
GOOGLE_CLIENT_SECRET=your-client-secret
GOOGLE_REDIRECT_URI=https://api.footprintmaps.com/import/google/oauth/callback

# Apple Push Notifications (APNs)
APNS_KEY_ID=your-key-id
APNS_TEAM_ID=your-team-id
APNS_BUNDLE_ID=com.wd.footprint.app
# Either provide path to .p8 file or the key content directly (as APNS_PRIVATE_KEY)
APNS_KEY_PATH=/path/to/AuthKey.p8
