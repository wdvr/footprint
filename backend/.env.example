# AWS Configuration
AWS_REGION=us-east-1
DYNAMODB_TABLE=footprint-table-dev

# Environment
ENVIRONMENT=dev

# JWT Configuration
JWT_SECRET=change-this-to-a-secure-random-string-in-production

# Apple Sign In
APPLE_BUNDLE_ID=com.wouterdevriendt.footprint

# Google OAuth for Sign In (iOS client ID, no secret needed for mobile apps)
GOOGLE_CLIENT_ID=269334695221-sek2s7nqal8hmt2latimle4f8kgesg6l.apps.googleusercontent.com

# Google OAuth for Gmail/Calendar Import (Web App client with secret)
# This MUST match the client ID used in the iOS app's GoogleAuthManager.swift
GOOGLE_IMPORT_CLIENT_ID=269334695221-0h0nbiimdobmjefsi13dhvgpsidhk5hf.apps.googleusercontent.com
GOOGLE_IMPORT_CLIENT_SECRET=your-client-secret-from-google-cloud-console
GOOGLE_REDIRECT_URI=https://api.footprintmaps.com/import/google/oauth/callback

# Apple Push Notifications (APNs)
APNS_KEY_ID=your-key-id
APNS_TEAM_ID=your-team-id
APNS_BUNDLE_ID=com.wouterdevriendt.footprint
# Either provide path to .p8 file or the key content directly (as APNS_PRIVATE_KEY)
APNS_KEY_PATH=/path/to/AuthKey.p8
