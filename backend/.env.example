# AWS Configuration
AWS_REGION=us-east-1
DYNAMODB_TABLE=footprint-table-dev

# Environment
ENVIRONMENT=dev

# Google OAuth credentials (for Gmail/Calendar import)
GOOGLE_CLIENT_ID=your-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-client-secret
GOOGLE_REDIRECT_URI=com.wd.footprint.app:/oauth2callback

# Apple Push Notifications (APNs)
APNS_KEY_ID=your-key-id
APNS_TEAM_ID=your-team-id
APNS_BUNDLE_ID=com.wd.footprint.app
# Either provide path to .p8 file or the key content directly (as APNS_PRIVATE_KEY)
APNS_KEY_PATH=/path/to/AuthKey.p8
